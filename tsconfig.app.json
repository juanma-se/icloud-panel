/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "paths": {
      "@docs-components/*": ["./src/components/*"],
      "@app/*": ["src/app/*"],
      "@models/*": ["src/models/*"],
      "@assets/*": ["src/assets/*"],
      "@environments/*": ["src/environments/*"],
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
